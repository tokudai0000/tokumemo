<h1>ユーザー一覧</h1>
<% if user_signed_in? %>
  <% @users.each do |u| %>
  <p><a href="/users/<%= u.id %>"><%= u.email %></a></p>
  <hr>
  <% end %>
  <%= link_to "ログアウト", destroy_user_session_path, :method => :delete %>
<% else %>
  <%= link_to "ユーザー登録", new_user_registration_path %>
  <br>
  <%= link_to "ログイン", new_user_session_path %>
<% end %>